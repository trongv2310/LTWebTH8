@model List<BookStore.Models.Theloaitin>
@{
    ViewBag.Title = "Home Page";
}

<main>
    <table>
        <tr>
            <th>
                Ten The Loai
            </th>
            <th></th>
        </tr>
        @foreach(var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.Tentheloai)</td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.IDLoai }) |
                        @Html.ActionLink("Details", "Details", new { id = item.IDLoai }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.IDLoai })
                    </td>
                </tr>
            }
    </table>
</main>